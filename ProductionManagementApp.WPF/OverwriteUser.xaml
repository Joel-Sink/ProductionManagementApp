<Window x:Class="ProductionManagementApp.WPF.OverwriteUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProductionManagementApp.WPF"
        mc:Ignorable="d"
        Title="OverwriteUser" Height="200" Width="250">
    <Grid>
        <StackPanel>
            <TextBlock Margin="10" TextWrapping="Wrap" HorizontalAlignment="Center">User Already Exists. Overwrite User?</TextBlock>
            <Button x:Name="Yes" Margin="0 10" Click="Button_Click">Yes</Button>
            <Button Margin="0 10" Click="Button_Click">No</Button>
        </StackPanel>
    </Grid>
</Window>
